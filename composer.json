{
    "name": "ethanhann/rejson",
    "description": "PHP client library for the ReJSON Redis module.",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Ehann\\ReJson\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ehann\\Tests\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ethan Hann",
            "email": "ethanhann@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
      "php": ">=7",
        "ethanhann/redis-raw": "^0.2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1",
        "ukko/phpredis-phpdoc": "^2.2@dev",
        "mockery/mockery": "^0.9.9",
        "predis/predis": "^1.1",
        "friendsofphp/php-cs-fixer": "^2.2",
        "consolidation/robo": "^1.0",
        "monolog/monolog": "^1.23",
        "cheprasov/php-redis-client": "^1.8"
    }
}
